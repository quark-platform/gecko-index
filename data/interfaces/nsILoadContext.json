                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * True if the load context is content (as opposed to chrome).  This is\n   * determined based on the type of window the load is performed in, NOT based\n   * on any URIs that might be around.\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": "readonly",
                  "type": "boolean",
                  "name": "isContent"
                }
