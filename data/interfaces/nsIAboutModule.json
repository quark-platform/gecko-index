                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n     * A flag that indicates whether this about: URI doesn't want to be listed\n     * in about:about, especially if it's not useful without a query string.\n     */"
                  }
