      "name": "nsIGleanPing",
      "base": {
        "kind": "ifacebase",
        "extends": "nsISupports"
      },
      "body": {
        "kind": "ifacebody",
        "contents": {
          "kind": "ifacebody_$0",
          "contents": [
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * Collect and submit the ping for eventual upload.\n   *\n   * This will collect all stored data to be included in the ping.\n   * Data with lifetime `ping` will then be reset.\n   *\n   * If the ping is configured with `send_if_empty = false`\n   * and the ping currently contains no content,\n   * it will not be queued for upload.\n   * If the ping is configured with `send_if_empty = true`\n   * it will be queued for upload even if empty.\n   *\n   * Pings always contain the `ping_info` and `client_info` sections.\n   * See [ping sections](https://mozilla.github.io/glean/book/user/pings/index.html#ping-sections)\n   * for details.\n   *\n   * @param aReason - Optional. The reason the ping is being submitted.\n   *                  Must match one of the configured `reason_codes`.\n   */"
                  }
                ],
                "code": {
                  "kind": "func",
                  "attributes": null,
                  "return_type": "void",
                  "name": "submit",
                  "params": {
                    "kind": "paramlist",
                    "first_param": {
                      "kind": "param",
                      "attribute": {
                        "kind": "attributes",
                        "first_attribute": {
                          "kind": "attribute",
                          "name": "optional",
                          "param": null
                        },
                        "other_attributes": []
                      },
                      "refType": "in",
                      "type": "ACString",
                      "name": "aReason"
                    },
                    "other": []
                  },
                  "raises": null
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * **Test-only API**\n   *\n   * Register a callback to be called right before this ping is next submitted.\n   * The provided function is called exactly once before submitting.\n   *\n   * Note: The callback will be called on any call to submit.\n   * A ping might not be sent afterwards, e.g. if the ping is empty and\n   * `send_if_empty` is `false`.\n   *\n   * @param aCallback - The callback to call on the next submit.\n   */"
                  }
                ],
                "code": {
                  "kind": "func",
                  "attributes": null,
                  "return_type": "void",
                  "name": "testBeforeNextSubmit",
                  "params": {
                    "kind": "paramlist",
                    "first_param": {
                      "kind": "param",
                      "attribute": null,
                      "refType": "in",
                      "type": "nsIGleanPingTestCallback",
                      "name": "aCallback"
                    },
                    "other": []
                  },
                  "raises": null
                }
              }
            }
          ]
