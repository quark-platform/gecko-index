      "doc_comment": {
        "kind": "DOC_COMMENT",
        "contents": "/**\n * A scriptable interface for getting information about a BHR detected hang.\n * This is the type of the subject of the \"bhr-thread-hang\" observer topic.\n */"
      },
      "attrs": {
        "kind": "attributes",
        "first_attribute": {
          "kind": "attribute",
          "name": "scriptable",
          "param": null
        },
        "other_attributes": [
          {
            "kind": "attributes_$0",
