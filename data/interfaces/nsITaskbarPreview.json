      "name": "nsITaskbarPreview",
      "base": {
        "kind": "ifacebase",
        "extends": "nsISupports"
      },
      "body": {
        "kind": "ifacebody",
        "contents": {
          "kind": "ifacebody_$0",
          "contents": [
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The controller for this preview. A controller is required to provide\n   * the behavior and appearance of the taskbar previews. It is responsible for\n   * determining the size and contents of the preview, which buttons are\n   * displayed and how the application responds to user actions on the preview.\n   *\n   * Neither preview makes full use of the controller. See the documentation\n   * for nsITaskbarWindowPreview and nsITaskbarTabPreview for details on which\n   * controller methods are used.\n   *\n   * The controller is not allowed to be null.\n   *\n   * @see nsITaskbarPreviewController\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "nsITaskbarPreviewController",
                  "name": "controller"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The tooltip displayed above the preview when the user hovers over it\n   *\n   * Default: an empty string\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "AString",
                  "name": "tooltip"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * Whether or not the preview is visible.\n   *\n   * Changing this option is expensive for tab previews since toggling this\n   * option will destroy/create the proxy window and its registration with the\n   * taskbar. If any step of that fails, an exception will be thrown.\n   *\n   * For window previews, this operation is very cheap.\n   *\n   * Default: false\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "boolean",
                  "name": "visible"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * Gets/sets whether or not the preview is marked active (selected) in the\n   * taskbar.\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "boolean",
                  "name": "active"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * Invalidates the taskbar's cached image of this preview, forcing a redraw\n   * if necessary\n   */"
                  }
                ],
                "code": {
                  "kind": "func",
                  "attributes": null,
                  "return_type": "void",
                  "name": "invalidate",
                  "params": null,
                  "raises": null
                }
              }
            }
          ]
        }
