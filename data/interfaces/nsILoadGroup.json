    {
      "kind": "interface_main",
      "doc_comment": {
        "kind": "DOC_COMMENT",
        "contents": "/**\n * A load group maintains a collection of nsIRequest objects.\n * This is used in lots of places where groups of requests need to be tracked.\n * For example, Document::mDocumentLoadGroup is used to track all requests\n * made for subdocuments in order to track page load progress and allow all\n * requests made on behalf of the document to be stopped, etc.\n */"
      },
      "attrs": {
        "kind": "attributes",
        "first_attribute": {
          "kind": "attribute",
          "name": "builtinclass",
          "param": null
        },
        "other_attributes": [
          {
            "kind": "attributes_$0",
