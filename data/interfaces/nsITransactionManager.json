                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * Sets the maximum number of transaction items the transaction manager will\n   * maintain at any time. This is commonly referred to as the number of levels\n   * of undo.\n   * @param aMaxCount A value of -1 means no limit. A value of zero means the\n   * transaction manager will execute each transaction, then immediately release\n   * all references it has to the transaction without pushing it on the undo\n   * stack. A value greater than zero indicates the max number of transactions\n   * that can exist at any time on both the undo and redo stacks. This method\n   * will prune the necessary number of transactions on the undo and redo\n   * stacks if the value specified is less than the number of items that exist\n   * on both the undo and redo stacks.\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "long",
                  "name": "maxTransactionCount"
                }
