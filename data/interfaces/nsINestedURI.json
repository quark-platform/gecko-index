                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The inner URI for this nested URI.  This must not return null if the\n   * getter succeeds; URIs that have no inner must not QI to this interface.\n   * Dynamically changing whether there is an inner URI is not allowed.\n   *\n   * Modifying the returned URI must not in any way modify the nested URI; this\n   * means the returned URI must be either immutable or a clone.\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": "readonly",
                  "type": "nsIURI",
                  "name": "innerURI"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The innermost URI for this nested URI.  This must not return null if the\n   * getter succeeds.  This is equivalent to repeatedly calling innerURI while\n   * the returned URI QIs to nsINestedURI.\n   *\n   * Modifying the returned URI must not in any way modify the nested URI; this\n   * means the returned URI must be either immutable or a clone.\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": "readonly",
                  "type": "nsIURI",
                  "name": "innermostURI"
                }
              }
            }
          ]
        }
