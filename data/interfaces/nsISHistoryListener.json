              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * Called before navigating to a session history entry by index, for example,\n   * when nsIWebNavigation::gotoIndex() is called.\n   */"
                  }
                ],
                "code": {
                  "kind": "func",
                  "attributes": null,
