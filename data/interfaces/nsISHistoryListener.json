                  "name": "OnHistoryPurge",
                  "params": {
                    "kind": "paramlist",
                    "first_param": {
                      "kind": "param",
                      "attribute": null,
                      "refType": "in",
                      "type": "long",
                      "name": "aNumEntries"
                    },
                    "other": []
                  },
                  "raises": null
                }
              }
