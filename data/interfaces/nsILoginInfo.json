          "contents": [
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * A string to display to the user for the origin which includes the httpRealm,\n   * where applicable.\n   * e.g. \"site.com\", \"site.com:1234\", or \"site.com (My Secure Realm)\"\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": "readonly",
                  "type": "AString",
                  "name": "displayOrigin"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The origin the login applies to.\n   *\n   * For example,\n   * \"https://site.com\", \"http://site.com:1234\", \"ftp://ftp.site.com\",\n   * \"moz-proxy://127.0.0.1:8888, \"chrome://FirefoxAccounts\", \"file://\".\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "AString",
                  "name": "origin"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The origin the login applies to, incorrectly called a hostname.\n   * @deprecated in favor of `origin`\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "AString",
                  "name": "hostname"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The origin a form-based login was submitted to.\n   *\n   * For logins obtained from HTML forms, this field is the origin of the |action|\n   * attribute from the |form| element. For\n   * example \"http://www.site.com\". [Forms with no |action| attribute\n   * default to submitting to their origin URL, so we store that.]\n   *\n   * For logins obtained from a HTTP or FTP protocol authentication,\n   * this field is NULL.\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "AString",
                  "name": "formActionOrigin"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The origin a form-based login was submitted to, incorrectly referred to as a URL.\n   * @deprecated in favor of `formActionOrigin`\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "AString",
                  "name": "formSubmitURL"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The HTTP Realm a login was requested for.\n   *\n   * When an HTTP server sends a 401 result, the WWW-Authenticate\n   * header includes a realm to identify the \"protection space.\" See\n   * RFC2617. If the response sent has a missing or blank realm, the\n   * hostname is used instead.\n   *\n   * For logins obtained from HTML forms, this field is NULL.\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "AString",
                  "name": "httpRealm"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The username for the login.\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "AString",
                  "name": "username"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The |name| attribute for the username input field.\n   *\n   * For logins obtained from a HTTP or FTP protocol authentication,\n   * this field is an empty string.\n   *\n   * @note This attribute is currently saved but not used.\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "AString",
                  "name": "usernameField"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The password for the login.\n   */"
                  }
                ],
                "code": {
                  "kind": "attribute_code",
                  "attributes": null,
                  "readonly": null,
                  "type": "AString",
                  "name": "password"
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * The |name| attribute for the password input field.\n   *\n   * For logins obtained from a HTTP or FTP protocol authentication,\n   * this field is an empty string.\n   *\n   * @note This attribute is currently saved but not used.\n   */"
                  }
