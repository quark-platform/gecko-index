                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * Add an item to the system status bar. Each item can only be present once,\n   * subsequent addItem calls with the same element will be ignored.\n   * The system status bar holds a strong reference to the added XUL menu\n   * element and the item will stay in the status bar until it is removed via\n   * a call to removeItem, or until the process shuts down.\n   * @param aDOMMenuElement A XUL menu element that contains a XUL menupopup\n   *                        with regular menu content. The menu's icon is put\n   *                        into the system status bar; clicking it will open\n   *                        a menu with the contents of the menupopup.\n   *                        The menu label is not shown.\n   */"
                  }
                ],
                "code": {
                  "kind": "func",
                  "attributes": null,
                  "return_type": "void",
                  "name": "addItem",
                  "params": {
                    "kind": "paramlist",
                    "first_param": {
                      "kind": "param",
                      "attribute": null,
                      "refType": "in",
                      "type": "Element",
                      "name": "aMenuElement"
                    },
                    "other": []
                  },
                  "raises": null
                }
              }
            },
            {
              "kind": "ifacebody_$0_$0",
              "contents": {
                "kind": "member_$0",
                "docComment": [
                  {
                    "kind": "DOC_COMMENT",
                    "contents": "/**\n   * Remove a previously-added item from the menu bar. Calling this with an\n   * element that has not been added before will be silently ignored.\n   * @param aDOMMenuElement The XUL menu element that you called addItem with.\n   */"
                  }
                ],
                "code": {
                  "kind": "func",
                  "attributes": null,
                  "return_type": "void",
                  "name": "removeItem",
                  "params": {
                    "kind": "paramlist",
                    "first_param": {
                      "kind": "param",
                      "attribute": null,
                      "refType": "in",
                      "type": "Element",
                      "name": "aMenuElement"
                    },
                    "other": []
                  },
                  "raises": null
                }
              }
            }
          ]
        }
